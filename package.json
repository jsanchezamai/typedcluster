{
  "name": "typedcluster",
  "displayName": "TypedCluster",
  "description": "Provides VS Code integration for dummy pub/sub cluster. Admin panel and operator desk. This package is Replit IA code generated, take at risk.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typedcluster.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
	"mergeconstribs": "npx typedcluster/src/utils/manifest-merger.ts",
    "vscode:prepublish": "npm run package",
    "compile": "mergeconstribs && webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint --fix src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
	"@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
	"@types/mqtt": "^0.0.34",
    "eslint": "^9.19.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "inquirer": "^12.4.2",
    "mqtt": "^5.10.3",
    "typedoc": "^0.27.9",
    "winston": "^3.17.0"
  }
}
